

<%= form_for @jampad , remote: true do |f|  %>


  <% if @jampad.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@jampad.errors.count, "error") %> prohibited this post from being saved:</h2>

      <ul>
      <% @jampad.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div class="jampad-reg">
 <div class="edit_field">
<h2>Fill details </h2>
</div>


     <div class="edit_field">
    
    <%= f.email_field :Email , placeholder: "Email" %>

    </div>

    <div class="edit_field">

    
 <%= f.country_select(:country,  include_blank: true, priority_countries: ["GB", "US", "CA", "AU", "TH", "JP", "IN", "FR", "DE"] , prompt: true) %>

    </div>
 
     <div class="edit_field">
    
    <%= f.telephone_field :Phone , placeholder: "Phone" %>

    <i>Format : XXX-XXX-XXXX</i>

    </div>

     <div class="edit_field">
    
    <%= f.text_field :Jampad_Name , placeholder: "Jampad Name" %>

    </div>

       <div class="edit_field">
    
    <%= f.text_field :city, placeholder: "City" %>

    </div>

       <div class="edit_field">
    
    <%= f.text_field :state , placeholder: "state" %>

    </div>

  
 

  <div class="edit_field">
    <%= f.submit %>
  </div>

   </div>
   </div>

<% end %>



